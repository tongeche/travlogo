{
  "name": "travlogo",
  "version": "1.0.0",
  "description": "Your One-Stop Travel and Booking Website",
  "main": "index.js",
  "scripts": {    
 
    "start": "npm run watch:tailwind",
    "build:css": "tailwindcss -i ./css/src/styles.css -o ./css/main.css --minify",
    "build:js": "esbuild js/main.js --bundle --outfile=./js/main.js --minify --allow-overwrite",
    "build": "npm run build:css && npm run build:js",    
    "dev:css": "tailwindcss -i ./css/src/styles.css -o ./css/main.css --watch"
  },
  "keywords": [
    "travel",
    "booking",
    "hotels",
    "flights",
    "cruises",
    "tours",
    "experiences",
    "restaurants",
    "blog",
    "affiliate",
    "tailwind",
    "netlify"
  ],
  "author": "Timothy Ongeche",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^10.4.21",
    "esbuild": "^0.25.6",
    "netlify-cli": "^22.2.2",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.7.2",
    "@supabase/supabase-js": "^2.51.0",
    "alpinejs": "^3.14.9",
    "dotenv": "^17.2.0",
    "node-fetch": "^3.3.2"
  },
  "type": "module"
}
